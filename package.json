    "homefront": "^1.3.2",
    "typer": "^1.1.0"
  },
  "devDependencies": {
    "spoonx-tools": "^1.0.0-0"
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-datatable/columns-filter",
